<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Page Object Model Basics">


  <!-- The Listener is used for generating Reports using the ReportNG jar files  -->
  <listeners>
  	<listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
  	<listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
  	<listener class-name="com.w2a.listeners.CustomListeners"></listener>
  </listeners>
  
  
  
  
  <test name="Login Test">
    <classes>
      <class name="com.w2a.testcases.LoginTest"/>
    </classes>
  </test> <!-- Test -->
  
  <test name="Create Account Test">
    <classes>
      <class name="com.w2a.testcases.CreateAccountTest"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
